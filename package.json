{
	"name": "thingsdb-lang",
	"displayName": "ThingsDB Lang",
	"description": "Syntax highlighting for ThingsDB",
	"version": "0.1.7",
	"publisher": "joente78",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "thingsdb",
				"aliases": [
					"ThingsDB",
					"thingsdb"
				],
				"extensions": [
					".ti"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "thingsdb",
				"scopeName": "source.ti",
				"path": "./syntaxes/thingsdb.tmLanguage.json"
			}
		]
	},
